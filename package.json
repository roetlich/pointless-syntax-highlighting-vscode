{
  "name": "pointless-syntax-highlighting",
  "displayName": "Pointless Syntax Highlighting",
  "description": "Syntax highlighting for the functional language Pointless",
  "version": "0.0.2",
  "license": "WTFPL",
  "publisher": "pointless-vscode-publisher",
  "repository": {
    "type": "git",
    "url": "https://github.com/roetlich/pointless-syntax-highlighting-vscode"
  },
  "scripts": {
    "copy-to-vscode": ""
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pointless",
        "aliases": [
          "Pointless",
          "pointless"
        ],
        "extensions": [
          ".ptls"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pointless",
        "scopeName": "source.ptls",
        "path": "./syntaxes/pointless.tmLanguage.json"
      }
    ]
  }
}
